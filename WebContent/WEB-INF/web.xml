<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>intake_2_0</display-name>
  
  <servlet>
        <servlet-name>SecureLogin</servlet-name>
        <servlet-class>org.faithfarm.intake.SecureLogin</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>IntakeServlet</servlet-name>
        <servlet-class>org.faithfarm.intake.IntakeServlet</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>CWTServlet</servlet-name>
        <servlet-class>org.faithfarm.cwt.CWTServlet</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>StudentServlet</servlet-name>
        <servlet-class>org.faithfarm.intake.StudentServlet</servlet-class>
  </servlet>
    <servlet>
        <servlet-name>GridServlet</servlet-name>
        <servlet-class>org.faithfarm.intake.GridServlet</servlet-class>
  </servlet> 
  <servlet-mapping>
	    <servlet-name>GridServlet</servlet-name>
	    <url-pattern>/grid</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	    <servlet-name>SecureLogin</servlet-name>
	    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	    <servlet-name>CWTServlet</servlet-name>
	    <url-pattern>/cwt</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	    <servlet-name>StudentServlet</servlet-name>
	    <url-pattern>/student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	    <servlet-name>IntakeServlet</servlet-name>
	    <url-pattern>/intake</url-pattern>
  </servlet-mapping>
	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>   
  </welcome-file-list>
</web-app>